LICENSE="BSD"
WEB_SITE="http://www.slitaz.org/"

DEPENDS="sane-backends tazpanel"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/var/www/tazpanel
	cp $stuff/* $fs/var/www/tazpanel/
}
