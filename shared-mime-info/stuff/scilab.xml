<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-scilab-sci">
		<sub-class-of type="text/plain"/>
		<comment>Scilab macro</comment>
		<comment xml:lang="ru">макрос Scilab</comment>
		<glob pattern="*.sci"/>
	</mime-type>
	<mime-type type="application/x-scilab-sce">
		<sub-class-of type="text/plain"/>
		<comment>Scilab script</comment>
		<comment xml:lang="ru">скрипт Scilab</comment>
		<glob pattern="*.sce"/>
	</mime-type>
	<mime-type type="application/x-scilab-tst">
		<sub-class-of type="application/sce"/>
		<comment>Scilab test</comment>
		<comment xml:lang="ru">тест Scilab</comment>
		<glob pattern="*.tst"/>
	</mime-type>
	<mime-type type="application/x-scilab-dem">
		<sub-class-of type="application/sce"/>
		<comment>Scilab demo</comment>
		<comment xml:lang="ru">демонстрация Scilab</comment>
		<glob pattern="*.dem"/>
	</mime-type>
	<mime-type type="application/x-scilab-xcos">
		<sub-class-of type="text/xml"/>
		<comment>Scilab/Xcos schema</comment>
		<comment xml:lang="ru">схема Scilab/Xcos</comment>
		<glob pattern="*.xcos"/>
	</mime-type>
	<mime-type type="application/x-scilab-zcos">
		<sub-class-of type="application/zip"/>
		<comment>Scilab/Xcos compressed schema</comment>
		<comment xml:lang="ru">сжатая схема Scilab/Xcos</comment>
		<glob pattern="*.zcos"/>
	</mime-type>
	<mime-type type="application/x-scilab-sod">
		<sub-class-of type="application/x-hdf5"/>
		<comment>Scilab open data format (hdf5 based)</comment>
		<comment xml:lang="ru">формат открытых данных Scilab (основан на hdf5)</comment>
		<glob pattern="*.sod"/>
	</mime-type>
	<!-- Deprecated -->
	<mime-type type="application/x-scilab-bin">
		<sub-class-of type="application/octet-stream"/>
		<comment>Scilab binary</comment>
		<comment xml:lang="ru">бинарный файл Scilab</comment>
		<glob pattern="*.bin"/>
	</mime-type>
	<mime-type type="application/x-scilab-cosf">
		<sub-class-of type="application/x-scilab-sce"/>
		<comment>Scicos schema</comment>
		<comment xml:lang="ru">схема Scicos</comment>
		<glob pattern="*.cosf"/>
	</mime-type>
	<mime-type type="application/x-scilab-cos">
		<sub-class-of type="application/x-scilab-bin"/>
		<comment>Scicos binary schema</comment>
		<comment xml:lang="ru">бинарная схема Scicos</comment>
		<glob pattern="*.cos"/>
	</mime-type>
</mime-info>
