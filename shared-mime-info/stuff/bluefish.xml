<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-bluefish-project">
		<comment>Bluefish project file</comment>
		<comment xml:lang="ca">fitxer de projecte Bluefish</comment>
		<comment xml:lang="cs">projektový soubor Bluefish</comment>
		<comment xml:lang="da">projektfil for Bluefish</comment>
		<comment xml:lang="de">Bluefish Projektdatei</comment>
		<comment xml:lang="en">Bluefish project file</comment>
		<comment xml:lang="es">archivo de proyecto Bluefish</comment>
		<comment xml:lang="eu">Bluefish proiektuaren fitxategia</comment>
		<comment xml:lang="fr">fichier de projet Bluefish</comment>
		<comment xml:lang="gl">Bluefish project bestand</comment>
		<comment xml:lang="hu">Bluefish projekt fájl</comment>
		<comment xml:lang="it">file di progetto di Bluefish</comment>
		<comment xml:lang="ja">Bluefish プロジェクトファイル</comment>
		<comment xml:lang="ko">Bluefish 프로젝트 파일</comment>
		<comment xml:lang="nb">Bluefish prosjektfil</comment>
		<comment xml:lang="nl">Bluefish project bestand</comment>
		<comment xml:lang="nn">Bluefish prosjektfil</comment>
		<comment xml:lang="pt">ficheiro de projecto do Bluefish</comment>
		<comment xml:lang="pt_BR">arquivo de projeto Bluefish</comment>
		<comment xml:lang="ro">fisier proiect Bluefish</comment>
		<comment xml:lang="ru">файл проекта Bluefish</comment>
		<comment xml:lang="sk">súbor Bluefish projektu</comment>
		<comment xml:lang="uk">файл проекту Bluefish</comment>
		<comment xml:lang="zh_CN">Bluefish 项目文件</comment>
		<sub-class-of type="text/plain"/>
		<glob pattern="*.bfproject"/>
	</mime-type>

	<mime-type type="application/x-bluefish-language2">
		<comment>Bluefish language definition file</comment>
		<comment xml:lang="ca">fitxer de definició de la llengua de Bluefish</comment>
		<comment xml:lang="cs">soubor Bluefish s definicí jazyka</comment>
		<comment xml:lang="da">sprogdefinitionsfil for Bluefish</comment>
		<comment xml:lang="de">Bluefish Sprach- und Syntaxunterstützung</comment>
		<comment xml:lang="en">Bluefish language definition file</comment>
		<comment xml:lang="es">archivo de definición de idioma de Bluefish</comment>
		<comment xml:lang="eu">Bluefish hizkuntzaren definizioaren fitxategia</comment>
		<comment xml:lang="fr">fichier de définition de language Bluefish</comment>
		<comment xml:lang="gl">Bluefish taal definitie bestand</comment>
		<comment xml:lang="hu">Bluefish language definition file</comment>
		<comment xml:lang="it">file di definizione linguaggio di Bluefish</comment>
		<comment xml:lang="ja">Bluefish 言語定義ファイル</comment>
		<comment xml:lang="ko">Bluefish 언어 정의 파일</comment>
		<comment xml:lang="nb">språkdefinisjonsfil for Bluefish</comment>
		<comment xml:lang="nl">Bluefish taal definitie bestand</comment>
		<comment xml:lang="nn">språkdefinisjonsfil for Bluefish</comment>
		<comment xml:lang="pt">ficheiro de definição de linguagem do Bluefish</comment>
		<comment xml:lang="pt_BR">arquivo de definição de linguagem do Bluefish</comment>
		<comment xml:lang="ro">fisier definire limba</comment>
		<comment xml:lang="ru">файл определений языка Bluefish</comment>
		<comment xml:lang="uk">файл визначення мов Bluefish</comment>
		<comment xml:lang="zh_CN">Bluefish 语言定义文件</comment>
		<sub-class-of type="application/xml"/>
		<magic priority="90">
			<match value="&lt;bflang" type="string" offset="0:1024"/>
			<match value="&lt;bfinc" type="string" offset="0:1024"/>
		</magic>
		<glob pattern="*.bflang2"/>
		<glob pattern="*.bfinc"/>
		<root-XML namespaceURI="http://bluefish.openoffice.nl/bflang2" localName="bflang"/>
	</mime-type>
</mime-info>
