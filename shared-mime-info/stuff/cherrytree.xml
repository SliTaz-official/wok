<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/cherrytree-ctd">
		<comment>CherryTree XML Document</comment>
		<comment xml:lang="ru">XML-документ CherryTree</comment>
		<glob pattern="*.ctd"/>
	</mime-type>

	<mime-type type="application/cherrytree-ctz">
		<comment>CherryTree XML Zipped</comment>
		<comment xml:lang="ru">сжатый XML-документ CherryTree</comment>
		<glob pattern="*.ctz"/>
	</mime-type>

	<mime-type type="application/cherrytree-ctb">
		<comment>CherryTree SQLite Document</comment>
		<comment xml:lang="ru">SQLite-документ CherryTree</comment>
		<glob pattern="*.ctb"/>
	</mime-type>

	<mime-type type="application/cherrytree-ctx">
		<comment>CherryTree SQLite Zipped</comment>
		<comment xml:lang="ru">сжатый SQLite-документ CherryTree</comment>
		<glob pattern="*.ctx"/>
	</mime-type>
</mime-info>
