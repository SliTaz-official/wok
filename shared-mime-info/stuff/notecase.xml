<?xml version="1.0"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/notecase-plain">
		<comment>NoteCase unencrypted document</comment>
		<comment xml:lang="ru">документ NoteCase</comment>
		<magic priority="80">
			<match type="string" offset="1" value="!DOCTYPE NoteCase-File"/>
		</magic>
		<glob pattern="*.ncd"/>
	</mime-type>

	<mime-type type="application/notecase-enc">
		<comment>NoteCase encrpyted document</comment>
		<comment xml:lang="ru">зашифрованный документ NoteCase</comment>
		<magic priority="80">
			<match type="string" offset="0" value="NOTECASE"/>
		</magic>
		<glob pattern="*.nce"/>
	</mime-type>
</mime-info>
