	<mime-type type="text/x-tazpkg-receipt">
		<comment>TazPkg recipe file</comment>
		<comment xml:lang="de">die Rezept den TazPkg</comment>
		<comment xml:lang="es">la receta para TazPkg</comment>
		<comment xml:lang="fr">fichier de recette pour TazPkg</comment>
		<comment xml:lang="pt">receita para TazPkg</comment>
		<comment xml:lang="ru">файл рецепта TazPkg</comment>
		<sub-class-of type="application/x-shellscript"/>
		<generic-icon name="text-x-script"/>
		<magic priority="50">
			<match value="# SliTaz package receipt." type="string" offset="0"/>
		</magic>
		<glob pattern="receipt"/>
	</mime-type>
	<mime-type type="text/x-flavor">
		<comment>TazLiTo flavor file</comment>
		<comment xml:lang="ru">файл редакции TazLiTo</comment>
		<sub-class-of type="application/gzip"/>
		<generic-icon name="package-x-generic"/>
		<magic priority="50">
			<match value=".desc" type="string" offset="110:256">
				<match value="Flavor" type="string" offset="118:256"/>
			</match>
		</magic>
		<glob pattern="*.flavor"/>
	</mime-type>
