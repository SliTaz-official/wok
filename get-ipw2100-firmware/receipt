# SliTaz package receipt.

PACKAGE="get-ipw2100-firmware"
VERSION="1.0"
CATEGORY="non-free"
SHORT_DESC="Get Intel PRO/Wireless 2100 firmware."
MAINTAINER="pascal.bellard@slitaz.org"
WEB_SITE="http://ipw2100.sourceforge.net//"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/$PACKAGE $fs/usr/bin
}
