# SliTaz package receipt.

PACKAGE="get-prince"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="formatter for converting XML and HTML into PDF."
MAINTAINER="rocky@slitaz.org"
LICENSE="BSD"
WEB_SITE="http://www.princexml.com/"
TAGS="utilities"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
    mkdir -p $fs/usr/bin
    cp stuff/get-prince $fs/usr/bin
}

