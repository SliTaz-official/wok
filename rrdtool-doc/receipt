# SliTaz package receipt.

PACKAGE="rrdtool-doc"
VERSION="1.7.2"
CATEGORY="development"
SHORT_DESC="Data logging and graphing system for time series data - documentation."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="GPL2"
WEB_SITE="https://oss.oetiker.ch/rrdtool"

WANTED="rrdtool"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/share

	cp -a $install/usr/share/rrdtool	$fs/usr/share
	cp -a $install/usr/share/doc		$fs/usr/share
}
