# SliTaz package receipt.

PACKAGE="get-xlite"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Get Xlite soft phone."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://web.archive.org/web/20120621050710/http://www.counterpath.net/"
TAGS="softphone telephony"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-xlite $fs/usr/bin
}

