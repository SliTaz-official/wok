# SliTaz package receipt.

PACKAGE="squid-langpack"
VERSION="3.0.STABLE25"
CATEGORY="network"
SHORT_DESC="Web/ftp proxy language pack."
MAINTAINER="pascal.bellard@slitaz.org"
WEB_SITE="http://www.squid-cache.org/"
WANTED="squid"
DEPENDS="squid"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/share
	cp -a $_pkg/usr/share/squid/errors $fs/usr/share
	rm -rf $fs/usr/share/squid/errors/English
	rm -rf $fs/usr/share/squid/errors/French
}

