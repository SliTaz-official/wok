# SliTaz package receipt.

PACKAGE="libplayer-python"
VERSION="1.0.0"
CATEGORY="multimedia"
SHORT_DESC="Python binding for libplayer. "
MAINTAINER="pankso@slitaz.org"
WANTED="libplayer"
DEPENDS="libplayer python"
WEB_SITE="http://libplayer.geexbox.org/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $_pkg/usr/lib/python* $fs/usr/lib
}
