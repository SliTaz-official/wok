# SliTaz package receipt.

PACKAGE="get-algobox"
VERSION="1.01"
CATEGORY="misc"
SHORT_DESC="Get the Algorithm creation tool for education."
MAINTAINER="maintainer@slitaz.org"
LICENSE="BSD"
WEB_SITE="https://www.xm1math.net/algobox/"

# fetches latest version only!
# available versions are amd64 only!

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-algobox	$fs/usr/bin
}
