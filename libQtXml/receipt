# SliTaz package receipt.

PACKAGE="libQtXml"
VERSION="4.5.0"
CATEGORY="development"
SHORT_DESC="Qt Xml library"
MAINTAINER="allan316@gmail.com"
DEPENDS="libQtCore libQtNetwork"
TARBALL="$PACKAGE-$VERSION.tar.gz"
WEB_SITE="http://www.trolltech.com"
WANTED="qt-x11-opensource-src"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $_pkg/usr/lib/libQtXml.so* $fs/usr/lib
	cp -a $_pkg/usr/lib/libQtXmlPatterns.so* $fs/usr/lib
}

