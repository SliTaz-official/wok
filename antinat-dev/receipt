# SliTaz package receipt.

PACKAGE="antinat-dev"
VERSION="0.90"
CATEGORY="development"
SHORT_DESC="Flexible SOCKS server & client library for proxy-based applications, development files."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="GPL2"
WEB_SITE="http://antinat.sourceforge.net/"

WANTED="antinat"

DEPENDS="antinat expat-dev"

# Rules to gen a SliTaz package suitable for Tazpkg.   
genpkg_rules()                                      
{
	mkdir -p $fs/usr/lib
	cp -a $install/usr/include $fs/usr
	cp -a $install/usr/lib/*a $fs/usr/lib
}
