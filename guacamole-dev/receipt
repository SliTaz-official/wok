# SliTaz package receipt.

PACKAGE="guacamole-dev"
VERSION="0.9.4"
CATEGORY="development"
SHORT_DESC="Cientless remote desktop gateway for VNC or RDP, development files"
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="MIT"
WEB_SITE="http://guac-dev.org/"
WANTED="guacamole"

DEPENDS="guacamole"

genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $install/usr/include $fs/usr
	cp -a $install/usr/lib/*a $fs/usr/lib
}
