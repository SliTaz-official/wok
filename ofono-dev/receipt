# SliTaz package receipt.

PACKAGE="ofono-dev"
VERSION="1.12"
CATEGORY="network"
SHORT_DESC="Stack for mobile telephony (GSM/UMTS) - dev files"
MAINTAINER="slaxemulator@gmail.com"
WANTED="ofono"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr
	cp -a $_pkg/usr/include $fs/usr
}
