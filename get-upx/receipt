# SliTaz package receipt.

PACKAGE="get-upx"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Get UPX executable packer."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="http://upx.sourceforge.net/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-upx $fs/usr/bin
}

