# SliTaz package receipt.

PACKAGE="blinder-theme"
VERSION="1.0.2"
CATEGORY="x-window"
SHORT_DESC="SliTaz Blinder theme for GTK+2 and Openbox"
MAINTAINER="hackdorte@sapo.pt"
LICENSE="BSD"
WEB_SITE="http://people.slitaz.org/~leonardolaporte/"

DEPENDS="murrine"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/share/themes
	cp -a stuff/* $fs/usr/share/themes
}
