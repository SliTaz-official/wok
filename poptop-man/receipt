# SliTaz package receipt.

PACKAGE="poptop-man"
VERSION="1.3.4"
CATEGORY="network"
SHORT_DESC="PPTP server man pages."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="GPL2"
WEB_SITE="http://www.poptop.org/"
WANTED="poptop"
SOURCE="pptpd"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/
	cp -a $install/usr/share $fs/usr/
}

