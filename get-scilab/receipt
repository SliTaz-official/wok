# SliTaz package receipt.

PACKAGE="get-scilab"
VERSION="1.00"
CATEGORY="misc"
SHORT_DESC="Get scilab: Free Open Source Software for Numerical Computation."
MAINTAINER="pascal.bellard@slitaz.org"
WEB_SITE="http://www.scilab.org/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-scilab $fs/usr/bin
}

