# SliTaz package receipt.

PACKAGE="scrollkeeper-dev"
VERSION="0.3.14"
CATEGORY="utilities"
LICENSE="GPL3"
SHORT_DESC="A cataloging system for documentation on open systems."
MAINTAINER="yuripourre@gmail.com"
WEB_SITE="http://sourceforge.net/projects/scrollkeeper/"
WANTED="scrollkeeper"
DEPENDS="scrollkeeper"

genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $install/etc $fs
	cp -a $install/usr/lib/*.la $fs/usr/lib
}
