LICENSE="GPL2"
SHORT_DESC="frei0r-plugins devel files"
WEB_SITE="https://frei0r.dyne.org/"
WANTED="frei0r-plugins"

DEPENDS="frei0r-plugins"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr
	cp -a $install/usr/include $fs/usr
}
