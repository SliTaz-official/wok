# SliTaz package receipt.

PACKAGE="get-wfica"
VERSION="1.01"
CATEGORY="non-free"
SHORT_DESC="Get Citrix ICA Client."
MAINTAINER="pascal.bellard@slitaz.org"
WEB_SITE="http://www.citrix.com/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp $stuff/$PACKAGE $fs/usr/bin
}

