# SliTaz package receipt.

PACKAGE="discount-dev"
VERSION="2.1.7"
CATEGORY="development"
SHORT_DESC="a C implementation of the markdown spec - dev files"
MAINTAINER="tcg.thegamer@gmail.com"
LICENSE="BSD3"
WEB_SITE="http://www.pell.portland.or.us/~orc/Code/markdown/"
HOST_ARCH="i486 arm"
WANTED="discount"

DEPENDS="discount"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	cd $src
	tar xvf $src/build/discount.tgz -C $fs
	rm -rf $fs/usr/bin
}
