# SliTaz package receipt.

PACKAGE="qupzilla-locales"
VERSION="v1.4.3"
CATEGORY="x-window"
SHORT_DESC="Locales for the qupzilla browser"
MAINTAINER="claudinei@slitaz.org"
LICENSE="GPLv3"
WEB_SITE="https://github.com/QupZilla/qupzilla"

DEPENDS="qupzilla"
WANTED="qupzilla"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/share/$WANTED
	cp -a $install/usr/share/$WANTED/locale $fs/usr/share/$WANTED
}
