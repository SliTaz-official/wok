# SliTaz package receipt.

PACKAGE="get-wakoopa"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Wakoopa is the perfect place to discover software and games online."
MAINTAINER="rocky@slitaz.org"
WEB_SITE="http://wakoopa.com"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
    mkdir -p $fs/usr/bin
    cp stuff/get-wakoopa $fs/usr/bin
}
