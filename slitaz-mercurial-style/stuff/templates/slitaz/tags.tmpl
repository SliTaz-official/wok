{header}
<title>{repo|escape}: tags</title>
<link rel="alternate" type="application/atom+xml"
   href="{url}atom-tags" title="Atom feed for {repo|escape}: tags" />
<link rel="alternate" type="application/rss+xml"
   href="{url}rss-tags" title="RSS feed for {repo|escape}: tags" />
</head>
<body>

<!-- Header -->
<div id="header">
    <a href="http://hg.slitaz.org/"><img id="logo"
		src="{staticurl}logo.png" 
		title="hg.slitaz.org" alt="hg.slitaz.org" /></a>
    <p id="titre">#!/Hg/repos</p>
</div>

<!-- Content -->
<div id="content-full">

<!-- Block begin -->
<div class="block">
	<!-- Nav block begin -->
	<div id="block_nav">
		<h3><img src="{staticurl}network.png" alt="png" />Navigation</h3>
		<ul>
			<li><a href="{url}shortlog{sessionvars%urlparameter}">log</a></li>
			<li><a href="{url}graph{sessionvars%urlparameter}">graph</a></li>
			<li class="active">tags</li>
			<li><a href="{url}branches{sessionvars%urlparameter}">branches</a></li>
		</ul>
	<!-- Nav block end -->
	</div>
	<!-- Top block begin -->
	<div id="block_top">
		<h1>Project: {repo|escape}</h1>
		<p>
			This web interface lets you follow in real time all improvements,
			fixes and other changes made by SliTaz GNU/Linux contributors.
		</p>
		<h3>Search</h3>
		<form class="search" action="{url}log">
			{sessionvars%hiddenformentry}
			<p><input name="rev" id="search1" type="text" /></p>
			<div id="hint">find changesets by author, revision,
			files, or words in the commit message</div>
		</form>
	<!-- Top block end -->
	</div>
<!-- Block end -->
</div>

<h2><a href="{url}{sessionvars%urlparameter}">{repo|escape}</a> tags</h2>

<table class="bigtable">
<tr>
 <th>tag</th>
 <th>node</th>
</tr>
{entries%tagentry}
</table>

<!-- End of content -->
</div>

{footer}
