# SliTaz package receipt.

PACKAGE="gdal-bin"
VERSION="1.9.0"
CATEGORY="misc"
SHORT_DESC="GDAL Tools."
MAINTAINER="erjo@slitaz.org"
LICENSE="MIT"
WEB_SITE="http://www.gdal.org/"
WANTED="gdal"

DEPENDS="libgdal"
BUILD_DEPENDS=""


# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr
	cp -a $install/usr/bin $fs/usr
	rm -f $fs/usr/bin/gdal-config
}
