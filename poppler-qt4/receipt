# SliTaz package receipt.

PACKAGE="poppler-qt4"
VERSION="0.14.5"
CATEGORY="office"
SHORT_DESC="Poppler QT4 library."
MAINTAINER="pankso@slitaz.org"
WANTED="poppler"
DEPENDS="poppler libQtCore libQtXml libQtGui gcc-lib-base"
WEB_SITE="http://poppler.freedesktop.org/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/lib
	cp -a $_pkg/usr/lib/*qt4.so* $fs/usr/lib
}
