# SliTaz package receipt.

PACKAGE="linuxqq"
VERSION="v1.0.2-beta1"
CATEGORY="network"
SHORT_DESC="Instant messaging software from Tencent Inc."
MAINTAINER="lufeng369@gmail.com"
WEB_SITE="http://im.qq.com"
DEPENDS="gtk+ libsigc++"
TARBALL="${PACKAGE}-${VERSION}.i386.rpm"
#BUILD_DEPENDS=""

genpkg_rules()
{
   mkdir stuff/
        wget http://dl_dir.qq.com/linuxqq/$TARBALL -P stuff/
        rpm2cpio stuff/$TARBALL | (cd $fs && cpio -iudv)

}

