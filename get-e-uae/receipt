# SliTaz package receipt.

PACKAGE="get-e-uae"
VERSION="1.00"
CATEGORY="system-tools"
TAGS="emulator"
SHORT_DESC="Get Enhanced Ubiquitous Amiga Emulator."
MAINTAINER="maintainer@slitaz.org"
LICENSE="GPL2"
WEB_SITE="http://www.rcdrummond.net/uae/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-e-uae $fs/usr/bin
}
