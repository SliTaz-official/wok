# SliTaz package receipt.

PACKAGE="get-nz"
VERSION="1.00"
CATEGORY="non-free"
SHORT_DESC="Get nanozip archiver."
MAINTAINER="pascal.bellard@slitaz.org"
LICENSE="BSD"
WEB_SITE="http://www.nanozip.net/"

# Rules to gen a SliTaz package suitable for Tazpkg.
genpkg_rules()
{
	mkdir -p $fs/usr/bin
	cp stuff/get-nz $fs/usr/bin
}

