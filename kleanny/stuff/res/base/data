localdata="/var/www/cgi-bin/kleanny/res"         # APP resource directory.
localicon="/var/www/cgi-bin/kleanny/res/icon"    # APP icons directory.
localtheme="/var/www/cgi-bin/kleanny/res/theme"  # APP YAD theme directory.

localdir="/var/www/cgi-bin/kleanny"              # APP real directory.
localwww="http://localhost/cgi-bin/kleanny"      # Call to YAD browser.
tmpdir="/tmp/$APP_NAME"                          # Temporary directory.
userdir="/home/$USER"                            # Real user directory.

